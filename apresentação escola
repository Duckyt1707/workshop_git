programa
{
	
	funcao inicio()
	{
		cadeia AL[5]
          para(inteiro i=0;i<5;i++){
          	escreva("informe o nome do aluno: ")
          	leia(AL[i])
          }
          escreva("\nordem de apresentação:\n")
		para(inteiro i=0;i<5;i++){
			
			escreva(i+1,"-",AL[i],"\n")
			
		}
		escreva("\n")
		para(inteiro i=0;i<5;i++){
			escreva(AL[i]," esta apresentando\n")
			escreva(AL[i]," aprsentou o trabalho\n\n")
			
		}
		escreva("fim da apresentação, todos reprovaram")
	}
	
}
